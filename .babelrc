{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }
    ]
  ],
  "plugins": [
    "@vue/babel-plugin-jsx",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-object-rest-spread"
  ]
}
